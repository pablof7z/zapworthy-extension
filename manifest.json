{
    "name": "Zapworthy",
    "version": "1.1",
    "description": "Highlight the web",
    "permissions": ["activeTab"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://zapworthy.com; object-src 'self'",
    "manifest_version": 2
  }
